
public class Assessment {

	private int number;
	
	public static int isGoofy(int num)
	{
		int count = 0;
		int y = num - 10*(num/10);
		int index = 0;
		boolean x = true;
		
		while(x == true)
		{
			if(num % y == 0)
			{
				count += y;
			}
			index++;
		}
		
		return count;
	}
	
	public static int[] getSomeGoofyNumbers(int count)
	{
		
	}
	
	public static void main(String[] args) {

		System.out.println(isGoofy(12));
	}
	
}

